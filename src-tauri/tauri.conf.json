{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Hungry Noodle - Snake",
  "identifier": "com.ionaru.hungrynoodle",
  "version": "../package.json",
  "build": {
    "beforeDevCommand": "npm run start",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/hungry-noodle/browser"
  },
  "app": {
    "windows": [
      {
        "title": "hungry-noodle",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
